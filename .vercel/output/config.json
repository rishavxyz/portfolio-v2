{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/@\\/create\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/about\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/create\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/blog(?:\\/(.*?))?\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/download\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/rss\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}